#include<stdio.h>
#include<math.h>
void GPTB2(float a, float b, float c);
int main(){
    GPTB2(4, 1, 6);
    return 0;
}
void GPTB2(float a, float b, float c){
    float delta, x1, x2;
    delta= b*b - 4*a*c;
    printf("delta: %f", delta);
if(delta>0){
    x1 = (-b + sqrt(delta)/2*a);
    x2 = (-b - sqrt(delta)/2*a);
    print("x1: %f\n", x1);
    print("x2: %f\n", x2);
}else{
    if(delta==0){
    x1=x2=-b/2a;
    printf("nghiem kep x1: %f\n", x1);
}else{
    if(delta<0){
    printf("phuong trinh vo nghiem\n");
}
}

