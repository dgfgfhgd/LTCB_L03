#include <stdio.h>
void NhapMaTran(int a[][10], int m, int n){
for(int i=0; i<n; i++){
    for(int j=0; j<n; j++){
        printf("a[%d %d] ", i, j);
        scanf("%d", &a[i][j]);
    printf("\n");
    }
}
}
int main() {
    int a[10][10], m, n, i, j;
    printf("Nhap m=", m);
    scanf("%d",&m);
    printf("Nhap n=", n);
    scanf("%d", &n);
    NhapMaTran(a,m,n);

    return 0;
}
